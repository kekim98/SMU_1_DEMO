<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="example.com.smu_1_demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher3"
